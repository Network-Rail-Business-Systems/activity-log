{
    "name": "networkrailbusinesssystems/activity-log",
    "description": "Common activity log components for GOV.UK systems",
    "type": "library",
    "homepage": "https://github.com/Network-Rail-Business-Systems/activity-log",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NetworkRailBusinessSystems\\ActivityLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NetworkRailBusinessSystems\\ActivityLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NetworkRailBusinessSystems\\ActivityLog\\ActivityLogServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Calum Gray",
            "email": "calum.gray@networkrail.co.uk"
        },
        {
            "name": "Tahmid Uddin",
            "email": "tahmid.uddin@networkrail.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.3",
        "anthonyedmonds/govuk-laravel": "dev-feature/form-builder-v2",
        "illuminate/support": "^12",
        "spatie/laravel-activitylog": "^4"
    },
    "require-dev": {
        "anthonyedmonds/laravel-testing-traits": "^1",
        "brianium/paratest": "^7",
        "larastan/larastan": "^3",
        "laravel/pint": "^1",
        "networkrailbusinesssystems/badges": "^1",
        "orchestra/testbench": "^10",
        "phpunit/phpunit": "^12"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
